    "keywords": ["video","id","extract"],
    "homepage": "https://www.visionapps.cz",
    "license": "MIT",
    "authors": [
        {
            "name": "VisionApps s.r.o.",
            "email": "info@visionapps.cz",
            "homepage": "http://www.visionapps.cz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "codeception/codeception": "*",
        "friendsofphp/php-cs-fixer": "^1.12",
        "squizlabs/php_codesniffer": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "VideoIdExtractor\\": "src/VideoIdExtractor/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
